pipeline{
       agent any
       stages {
       
            stage('Compile'){
                  steps{ 
                    
                                echo "Compiled successfully"
                                 
                                 
                           }
                           
                  }
                  
                  
         stage('Unit'){
            
               steps{
                                sh"cat /etc/os-release"
                                
                                
                                }
                                
                                
                             }
                    
                    
                    
          stage('to run ansible pipeline'){
          
                 steps{        
                           sh "ansiblePlaybook installation: 'ansible', playbook: '/root/play.yml'"
                           
                           }
                           
                           }
          
                    }
                    
                    
                            
                            
        }
                            
                            
                            
                  
                  
                          
                  
                         
          
